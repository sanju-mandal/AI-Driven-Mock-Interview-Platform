  <script>
    const cardData = [
      {
        img: "recruitment-6838250_640.png",
        alt: "Feedback",
        title: "Personalized Feedback",
        desc: "Receive tailored advice to improve every answer you give."
      },
      {
        img: "mock_ai_int.png",
        alt: "Simulation",
        title: "Real-time Simulation",
        desc: "Experience interviews just like real ones, anytime, anywhere."
      },
      {
        img: "set_goal.png",
        alt: "Questions",
        title: "Expert-Level Questions",
        desc: "Practice with industry-standard questions curated by professionals."
      },
      {
        img: "practice.png",
        alt: "Growth",
        title: "Career Growth",
        desc: "Our tools are built to guide you toward long-term success."
      },
      {
        img: 'smartprep.png',
        alt: 'Smart Prep',
        title: 'Smart Prep',
        desc: 'Adaptive preparation materials based on your weak areas.'
      },
    ];
  
    const carousel = document.getElementById("carousel");
  
    function createCard(card) {
      const cardElement = document.createElement("div");
      cardElement.className = "card p-4 text-center bg-white rounded-xl shadow-[0_8px_30px_rgba(0,0,0,0.25)]";
      cardElement.innerHTML = `
        <img src="${card.img}" alt="${card.alt}" id="caceer">
        <h3 class="font-bold text-2xl">${card.title}</h3>
        <p class="text-gray-500 text-lg">${card.desc}</p>
      `;
      return cardElement;
    }
  
    // Add the cards twice to create infinite loop illusion
    for (let i = 0; i < 3; i++) {
      cardData.forEach(card => {
        const cardEl = createCard(card);
        carousel.appendChild(cardEl);
      });
    }
  
    // Add animation class
    carousel.classList.add("animate-slide");
  </script> 
